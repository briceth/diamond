<div class="container">
  <div class="row">
    <div class="col-sm-4 col-sm-offset-4">
      <%= simple_form_for @user do |f| %>
        <%= f.input :bio %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :photos, as: :attachinary %>
        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
