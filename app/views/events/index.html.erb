<div class="container">
  <div class="col-md-4 col-md-offset-4">
    <% if @events.any? %>
      <% @events.each do |event| %>
      <div>
        <%= event.user.first_name || event.user.email %>
        <%= event.action %>
        <%= event.eventable_id %>
        <%= event.eventable_type %>
      </div>
      <% end %>
    <% end %>
  </div>
</div>


